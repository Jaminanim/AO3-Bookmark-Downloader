{
  "manifest_version": 3,
  "name": "AO3 Bookmarks Downloader v2",
  "version": "1.0.0",
  "description": "Download all your AO3 bookmarked works with respect for server limits",
  "permissions": [
    "downloads",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://archiveofourown.org/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}